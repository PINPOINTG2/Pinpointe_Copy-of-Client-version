<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Subscribers API">
	<test thread-count="5"
		name="Subscribers positive and Negative Run">
		<classes>
			<class name="subscribers_positive.AddSubscribers" />
			<class name="subscribers_positive.GetSubscribers" />
			<class name="subscribers_positive.UpdateSubscribers" />
			<class name="subscribers_positive.CheckListforContact" />
			<class name="subscribers_positive.Unsubscribe" />
			<class
				name="subscribers_positive.CheckListforUnsubscribedContact" />
			<class name="subscribers_positive.DeleteSubscriber" />
			<class name="subscribers_negative.AddSubscribers" />
			<class name="subscribers_negative.GetSubscribers" />
			<class name="subscribers_negative.UpdateSubscribers" />
			<class name="subscribers_negative.CheckListforContact" />
			<class name="subscribers_negative.Unsubscribe" />
			<class
				name="subscribers_negative.CheckListforUnsubscribedContact" />
			<class name="subscribers_negative.DeleteSubscribers" />
			<class name="subscribers_negative.AddMultipleSubscribers" />

		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Lists Positive and Negative Flow">
		<classes>
			<class name="lists_positive.FindListByListId" />
			<class name="lists_positive.FindListByName" />
			<class name="lists_positive.GetCustomFieldStructure" />
			<class name="lists_positive.GetListSubscribers" />
			<class name="lists_positive.GetUsersCustomFieldData" />
			<class name="lists_negative.FindList" />
			<class name="lists_negative.GetCustomFieldStructure" />
			<class name="lists_negative.GetListSubscribers" />
			<class name="lists_negative.GetUsersCustomFieldData" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Tags positive and negative Flow">
		<classes>
			<class name="tags_positive.CreateTags" />
			<class name="tags_positive.GetTags" />
			<class name="tags_positive.GetTagSubscribers" />
			<class name="tags_positive.TagSubscribers" />
			<class name="tags_positive.UntagSubscribers" />
			<class name="tags_negative.CreateTags" />
			<class name="tags_negative.GetTagSubscribers" />
			<class name="tags_negative.TagSubscribers" />
			<class name="tags_negative.UntagSubscribers" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
